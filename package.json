{
  "name": "vue-twzipcode",
  "version": "1.0.4",
  "description": "Rewrite jQuery-TWzipcode in Vue.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/CasperLaiTW/vue-twzipcode.git"
  },
  "main": "dist/vue-twzipcode.js",
  "scripts": {
    "build": "node_modules/.bin/webpack -p --config=webpack.production.config.babel.js",
    "lint": "node_modules/.bin/eslint src/** tests/**",
    "release": "standard-version --no-verify",
    "test": "karma start karma.conf.js"
  },
  "keywords": [
    "vuejs",
    "twzipcode"
  ],
  "author": "Casper Lai",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-vue": "^0.1.1",
    "function-bind": "^1.1.0",
    "ghooks": "^1.2.4",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "standard-version": "^2.3.1",
    "strip-loader": "^0.1.2",
    "validate-commit-msg": "^2.5.0",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^8.3.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "vue": "^1.0.21",
    "babel-runtime": "^6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "./node_modules/.bin/validate-commit-msg"
    }
  }
}
